{
  "name": "signal-garden",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:galaxy": "jest --testPathPatterns=GalaxyMapModular",
    "test:worklet": "jest --testPathPatterns=WorkletDataIsolation",
    "test:performance": "jest --testPathPatterns=Performance",
    "test:gesture": "jest --testPathPatterns=Gesture",
    "test:spatial": "jest --testPathPatterns=Spatial",
    "test:component": "jest --testPathPatterns=components",
    "test:util": "jest --testPathPatterns=utils",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "analyze:reanimated": "node scripts/analyze-reanimated-warnings.js",
    "dev:analyze": "npm run ios 2>&1 | node scripts/analyze-reanimated-warnings.js"
  },
